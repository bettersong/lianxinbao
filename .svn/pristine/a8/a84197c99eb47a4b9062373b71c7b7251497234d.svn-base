.header{
    width:100%;
    /* position:fixed; */
    top:0px;
    z-index:9;
}
.head{
     width:100%;
     height: 150px;
     background:url("../images/head1.png") no-repeat center/100% 100%;
     margin-top:0px;
     position:relative;
}
.head .h-inner{
    width:60%;
    height:50%;
    margin:auto;
    background:url("../images/tit.png") no-repeat center/70% 60%;
    position:absolute;
    top:20px;
    left:260px;
}
#menu{
    width:100%;
    height: 30px;
    background:url("../images/daohang.png") no-repeat center/100% 100%;
    margin-top:-30px;
    box-shadow: 0 2px 20px #f9b487;
}
#menu .nav{
    width:1079px;
    height:100%;
    margin:0 auto;
    list-style:none;
    display:flex;
    justify-content:space-around;
    padding:0px;
}
#nav .mainlevel{background:#ffe60c;float:left;border-right:1px solid #fff;width:140px;/*IE6 only*/}
.nav .nav_li{
    width: 100%;
    height:100%;
    border-right:1px solid #fff;
    text-align:center;
    align-items: center;
    position:relative;
    cursor:pointer;
}
.nav .nav_li:hover{
    background:#ffefd5;
}
.nav .nav_li:hover a{
    color:#333;
}
.nav .nav_li:hover ul li a{
    color:#fff;
}
// .nav a{
//     text-decoration: none;
//     color:#fff;
//     font-size:14px;
//     font-weight:bold;
// }
.nav_li a{
        text-decoration: none;
        color:#fff;
        display: inline-block;
        width: 100%;
        height: 30px;
        line-height: 30px;
        font-weight:500;
        blr:expression(this.onFocus=this.close());
        blr:expression(this.onFocus=this.blur());
        font-size: 15px;
    }
.nav li a:focus{
    -moz-outline-style: none;
    }
.nav li .sub-nav{
    padding:0px;
    position:absolute;
    top: 30px;
    left: 0;
    list-style:none;
    background-color:#b41d24;
    display: none;
    z-index:9;
    }
.nav li .sub-nav li{
    text-align:center;
    clear:left;
    width: 107px;
    height: 30px;
    line-height: 30px;
    position:relative;
    border-top: 1px solid #fff;
    }
.nav li .sub-nav li:hover{
        background:#ffefd5;
        color:#000;
    }
    .nav li .sub-nav li:hover a{

        color:#000;
    }
.nav li .sub-nav li a{
    height:34px;
    line-height:34px;
    width: 107px;
    padding:0;
    display:inline-block;
    font-size: 12px;
    }
.nav_bottom{
    width:100%;
    height: 40px;
    margin-top:180px;
    position:relative;
}
.nav_bottom .nav_bottom_left{
    position:absolute;
    height:100%;
    width: 256px;
    left: 386px;
}
.nav_bottom_left span{
    color:#8b1c3b;
    font-weight:600;
}
#time{
    width: 72%;
    font-size: 14px;
    float: right;
    margin-top: -30px;
}
.nav_bottom .nav_bottom_right{
    position:absolute;
    height:100%;
    width: 260px;
    right: 418px;
    display:flex;
    align-items: center;
}
.nav_bottom .nav_bottom_right .btn{
    width:270px;
    height:60%;
    border: 1px solid #eb4909;
    border-radius: 5px;
    background: #F9F0DA;
    color: #9E9C9C;
}
.nav_bottom .nav_bottom_right .search{
    width:40px;
    height:60%;
    background:#eb4909;
    outline:none;
    display: inline-block;
    // padding: .3em .5em;
    // background-image: linear-gradient(#ddd, #bbb);
    border: 1px solid rgba(0,0,0,.2);
    border-radius: .3em;
    box-shadow: 0 1px white inset;
    text-align: center;
    text-shadow: 0 1px 1px black;
}
.nav_bottom .nav_bottom_right .search:active{
    box-shadow: .05em .1em .2em rgba(0,0,0,.6) inset;
    border-color: rgba(0,0,0,.3);
    background: #eb4333;
}
.main{
    width: 1024px;
    margin:20px auto;
}
.main .m_top_title{
    width:100%;
    height:80px;
    margin:20px auto;
    background:url("../images/main_title.png") no-repeat center/100% 100%;
    position:relative;
}
.m_top_title .span_left{
    position: absolute;
    top: 20px;
    left: 120px;
    font-size: 16px;
    font-weight: 600;
    color: #fff;
}
.m_top_title .span_right{
    position: absolute;
    top: 60px;
    right: 130px;
    font-size: 25px;
    font-weight: 600;
    color: #b72028;
}
.main .m_top{
    width:100%;
    height: 280px;
    position:relative;
}
.main .m_top .m_banner{
    width:45%;
    height:100%;
    position:absolute;
    top:0px;
    left:0px;
}
.main .m_top .m_top_right{
    height:100%;
    width: 50%;
    position:absolute;
    top:0px;
    right:0px;
}
.main .m_top .m_top_right .m_top_right_title{
    width:100%;
    height:60px;
    border-bottom:1px dashed #ccc;
}
.m_top_right_title p{
    font-size:14px;
    color:#999;
}
.m_top_right_title a{
    text-decoration:none;
    color:#b72028;
}
.main .m_top .m_top_right .right_text_top a{
    display:block;
    width:90%;
    height: 40px;
    text-decoration:none;
    margin:5px auto;
    font-size: 16px;
    color:#b9262c;
    line-height:30px;
}
.main .m_top .m_top_right .right_text_mid{
    width:100%;
    margin-top:20px;
    position:relative;
}
.right_text_mid .mid_hot{
    width: 45px;
    height: 170px;
    position: absolute;
    top: 15px;
    left: 0px;
}
.right_text_mid .mid_hot span{
    font-size: 35px;
    color:#b9262c;
    font-weight:bold;
    font-family:"楷体";
}
.main .m_top .m_top_right .right_text_mid ul{
    position:absolute;
    top:0px;
    left:75px;
    width: 85%;
    height:100%;
    margin:auto;
    align-items: center;

}
.main .m_top .m_top_right .right_text_mid ul li{
    width: 100%;
    height: 27px;
    border-bottom:1px #ccc dashed;
    margin-bottom:5px;
}
.main .m_top .m_top_right .right_text_mid ul li a{
    width:280px;
    position:absolute;
    left: 5px;
    text-decoration:none;
    font-size: 14px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.main .m_top .m_top_right .right_text_mid ul li .hot{
    width:26px;
    height:18px;
    background: url("../images/new.png?a1") no-repeat center/100% 100%;
    float:right;
    margin-right: 60px;
}
.main .m_top .m_top_right .right_text_mid ul li span{
    position:absolute;
    font-size: 16px;
    right: 0px;
    color: #888;
}
.main .top_img{
    width: 1024px;
    height: 148px;
    margin-top:20px;
    position:relative;
    background:url("../images/center.png") no-repeat center/100% 100%;
}
.main .top_img .top_img_inner{
    width:60%;
    height:40px;
    position:absolute;
    top:40px;
    right:125px;
    background:url("../images/center1.png") no-repeat center/100% 100%;
}
.main .mid{
    width: 1024px;
    height: 320px;
    margin-top:20px;
    position:relative;
}
.main .mid .mid_one,.mid_two{
    width: 380px;
    height:100%;
    background:url("../images/lianxin-bg.png") no-repeat center/100% 100%;
    position:absolute;
    left:0px;
}
.main .mid .mid_two{
    width: 380px;
    height:100%;
    position:absolute;
    left: 390px;
}
.mid_one_head,.mid_two_head{
    width:100%;
    height: 40px;
}
 .mid_one_head_left,.mid_two_head_left{
    width:100%;
    height:100%;
    background:url("../images/lianxin.png") no-repeat center/100% 100%;
}
.mid_one_head_right{
    float:right;
    line-height:40px;

}
.mid_one_head_right a{
    text-decoration:none;
    font-size:15px;
    color:#b12112;
}
.mid_one_head_left span,.mid_two_head_left span{
    font-size: 18px;
    color:#fff;
    line-height: 40px;
    margin-left:40px;
    font-weight: bold;
}
.mid_one_head_center ul, .mid_two_head_center ul {
    width: 80%;
    height: 100%;
    margin: auto;
    align-items: center;
    margin-top: 13px;
    padding: 1px;
    /* margin: 0; */
}
.mid_one_head_center ul li,.mid_two_head_center ul li{
    width: 90%;
    height: 27px;
    border-bottom:1px #ccc dashed;
    margin-bottom:5px;
}
.mid_one_head_center ul li a,.mid_two_head_center ul li a{
    width:260px;
    position:absolute;
    left:40px;
    text-decoration:none;
    font-size: 13px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.mid_one_head_center ul li span,.mid_two_head_center ul li span{
    position:absolute;
    right:5px;
    font-size: 14px;
}
.main .mid .mid_three{
    width: 245px;
    height:100%;
    background:url("../images/gonggao1.png") no-repeat center/100% 100%;
    position:absolute;
    right:0px;
}
.main .mid .mid_three .mid_three_head{
    width:100%;
    height: 46px;
    margin-top:-5px;
    background:url("../images/gonggao.png") no-repeat center/100% 100%;
}
.mid_three_head span{
    line-height: 30px;
    margin-left: 95px;
    font-size: 18px;
}
.mid_three p{
    font-size: 13px;
    line-height: 31px;
    padding:5px;
}
.main .bottom{
    width: 1024px;
    height: 280px;
    margin-top:20px;
}
.bottom .bottom_head{
    width:100%;
    height: 40px;
    background:url("../images/bottom_head.png") no-repeat center/100% 100%;
}
.bottom_head span{
    line-height: 40px;
    font-size: 15px;
    color:#fff;
    margin-left: 60px;
}
.bottom_main{
    height: 220px;
    border:1px solid #f5db95;
}
.bottom_main ul{
    padding:0px;
    width: 98%;
    height:100%;
    margin:auto;
    list-style:none;
    display:flex;
    justify-content:space-around;
}
.bottom_main ul li{
    width: 235px;
    height:100%;
}
.bottom_main ul li .bottom_img{
    width: 235px;
    height:165px;
    margin:10px auto;
    overflow:hidden;
}
.bottom_img img{
    transition:.5s;
}
.bottom_img:hover img{
    transform:scale(1.2);
}
.bottom_main ul li span{
    display:block;
    width:100%;
    text-align:center;
    font-size: 15px;
}
.bottom_banner{
    width: 1024px;
    height: 150px;
    margin: 10px auto;
    position:relative;
    background:url("../images/bottom_banner.png") no-repeat center/100% 100%;
}
.bottom_banner_inner{
    width: 55%;
    height: 35%;
    position: absolute;
    top: 50px;
    right: 180px;
    background: url(../images/bottom_inner.png) no-repeat center/100% 100%;
}
.footer{
    width:100%;
    height: 150px;
    margin-top:10px;
    background:url(../images/footer.png) no-repeat center/100% 100%;
    position:relative;

}
.footer span{
    display:block;
    width:100%;
    text-align:center;
    position:absolute;
    top:70px;
    font-size: 16px;
}
.footer .footer_ask{
    width:100%;
    text-align:center;
    position:absolute;
    top:100px;
    font-size: 17px;
}
.active{
   width:215px;
   height: 40px;
   background:url("../images/BL_mid.png") no-repeat center/100% 100%;
   margin:20px auto;
   cursor:pointer;
   font-weight:bold;
}
.backTop{
    position:fixed;
    right:50px;
    bottom:60px;
    width:35px;
    height:35px;
    background:url("../images/backtop.png") no-repeat center/100% 100%;
    display:none;
    z-index:9;
}
