html,body{margin:0px;padding:0px;width:100%}
.header{
    width:100%;
    position:fixed;
    top:0px;
    z-index:9
}
.head{
     width:100%;
     height: 150px;
     background:url("../images/head1.png") no-repeat center/100% 100%;
     margin-top:0px;
     position:relative;
}
.head .h-inner{
    width:60%;
    height:50%;
    margin:auto;
    background:url("../images/tit.png") no-repeat center/70% 60%;
    position:absolute;
    top:20px;
    left:260px;
}
#menu{
    width:100%;
    height: 30px;
    background:url("../images/daohang.png") no-repeat center/100% 100%;
    margin-top:-30px;
}
#menu .nav{
    width:80%;
    height:100%;
    margin:auto;
    list-style:none;
    display:flex;
    justify-content:space-around;
    align-items: center;
}

.nav .nav_li{
    width: 100px;
    height:100%;
    border-right:1px solid #fff;
    text-align:center;
    align-items: center;
    float:left;
    position:relative;
}
// .nav a{
//     text-decoration: none;
//     color:#fff;
//     font-size:14px;
//     font-weight:bold;
// }
.nav_li a{
        text-decoration: none;
        color:#fff;
        height: 30px;
        line-height: 12px;
        font-weight:500;
        blr:expression(this.onFocus=this.close());
        blr:expression(this.onFocus=this.blur());
        display:inline-block;
        font-size: 15px;
    }
.nav li a:focus{
    -moz-outline-style: none;
    }
.nav li .sub-nav{
    padding:0px;
    position:absolute;
    top: 30px;
    left: -7px;
    list-style:none;
    background-color:#b41d24;
    display: none;
    padding: -3px;
    }
.nav li .sub-nav li{
    text-align:center;
    clear:left;
    width: 107px;
    height: 30px;
    line-height: 30px;
    position:relative;
    }
    .nav li .sub-nav li:hover{
        background:#82ce18;
    }
.nav li .sub-nav li a{
    height:34px;
    line-height:34px;
    width: 111px;
    padding:0;
    display:inline-block;
    font-size: 12px;
    }
.nav_bottom{
    width:100%;
    height: 40px;
    margin-top:180px;
    position:relative;
}
.nav_bottom .nav_bottom_left{
    position:absolute;
    height:100%;
    width: 256px;
    left: 386px;
}
.nav_bottom_left span{
    color:#8b1c3b;
    font-weight:600;
}
#time{
    width: 72%;
    font-size: 14px;
    float: right;
    margin-top: -30px;
}
.nav_bottom .nav_bottom_right{
    position:absolute;
    height:100%;
    width: 260px;
    right: 418px;
    display:flex;
    align-items: center;
}
.nav_bottom .nav_bottom_right .btn{
    width:270px;
    height:60%;
    border: 1px solid #eb4909;
    border-radius: 5px;
    background: #F9F0DA;
    color: #9E9C9C;
}
.nav_bottom .nav_bottom_right .search{
    width:40px;
    height:60%;
    background:#eb4909;
    outline:none;
    display: inline-block;
    // padding: .3em .5em;
    // background-image: linear-gradient(#ddd, #bbb);
    border: 1px solid rgba(0,0,0,.2);
    border-radius: .3em;
    box-shadow: 0 1px white inset;
    text-align: center;
    text-shadow: 0 1px 1px black;
}
.nav_bottom .nav_bottom_right .search:active{
    box-shadow: .05em .1em .2em rgba(0,0,0,.6) inset;
    border-color: rgba(0,0,0,.3);
    background: #eb4333;
}
.main{
    width: 70%;
    margin:20px auto;
}
.main .m_top{
    width:100%;
    height: 360px;
    position:relative;
}
.main .m_top .m_banner{
    width:45%;
    height:100%;
    position:absolute;
    top:0px;
    left:0px;
}
.main .m_top .m_top_right{
    height:100%;
    width: 50%;
    position:absolute;
    top:0px;
    right:0px;
    background:url("../images/tit1.png") no-repeat center/100% 100%;
}
.main .m_top .m_top_right .m_top_right_title{
    width:100%;
    height:45px;
    background:url("../images/banner1.png") no-repeat center/100% 100%;
    position:absolute;
    top:-3px;
    right:0;
}
.main .m_top .m_top_right .m_top_right_title .m_inner{
    width:40%;
    height:100%;
    position:absolute;
    left:-25px;
    top:-6px;
    text-align:center;
    line-height:45px;
    font-size: 16px;
    font-weight:bold;
}
.main .m_top .m_top_right .m_top_right_title .m_inner_right{
    position:absolute;
    right:40px;
    line-height:45px;
}
.main .m_top .m_top_right .m_top_right_title .m_inner_right a{
    text-decoration:none;
    font-size: 15px;
    color:#b12112;
}
.main .m_top .m_top_right .right_text_top{
    width:100%;
    height:50px;
    position:absolute;
    top:40px;

}
.main .m_top .m_top_right .right_text_top p{
    padding:15px;
    font-size: 16px;
    color:#b9262c;
    line-height:30px;
}
.main .m_top .m_top_right .right_text_mid{
    width:100%;
    position:absolute;
    top:160px;
}
.main .m_top .m_top_right .right_text_mid ul{
    width: 88%;
    height:100%;
    margin:auto;
    align-items: center;
}
.main .m_top .m_top_right .right_text_mid ul li{
    width:100%;
    height: 27px;
    border-bottom:1px #ccc dashed;
    margin-bottom:5px;
}
.main .m_top .m_top_right .right_text_mid ul li a{
    position:absolute;
    left: 21px;
    text-decoration:none;
    font-size: 14px;
}
.main .m_top .m_top_right .right_text_mid ul li .hot{
    width:30px;
    height:15px;
    background:url("../images/new.png") no-repeat center/100% 100%;
    float:right;
    margin-right: 100px;
}
.main .m_top .m_top_right .right_text_mid ul li span{
    position:absolute;
    left: 400px;
    font-size: 16px;
}
.main .top_img{
    width:100%;
    height: 148px;
    margin-top:20px;
    position:relative;
    background:url("../images/center.png") no-repeat center/100% 100%;
}
.main .top_img .top_img_inner{
    width:60%;
    height:40px;
    position:absolute;
    top:40px;
    right:125px;
    background:url("../images/center1.png") no-repeat center/100% 100%;
}
.main .mid{
    width:100%;
    height: 320px;
    margin-top:20px;
    position:relative;
}
.main .mid .mid_one,.mid_two{
    width: 360px;
    height:100%;
    background:url("../images/lianxin-bg.png") no-repeat center/100% 100%;
    position:absolute;
    left:0px;
}
.main .mid .mid_two{
    width: 360px;
    height:100%;
    position:absolute;
    left: 370px;
}
.mid_one_head,.mid_two_head{
    width:100%;
    height: 40px;
}
 .mid_one_head_left,.mid_two_head_left{
    width:100%;
    height:100%;
    background:url("../images/lianxin.png") no-repeat center/100% 100%;
}
.mid_one_head_left span,.mid_two_head_left span{
    font-size: 18px;
    color:#fff;
    line-height: 40px;
    margin-left:40px;
    font-weight: bold;
}
.mid_one_head_center ul, .mid_two_head_center ul {
    width: 80%;
    height: 100%;
    margin: auto;
    align-items: center;
    margin-top: 13px;
    padding: 1px;
    /* margin: 0; */
}
.mid_one_head_center ul li,.mid_two_head_center ul li{
    width: 90%;
    height: 27px;
    border-bottom:1px #ccc dashed;
    margin-bottom:5px;
}
.mid_one_head_center ul li a,.mid_two_head_center ul li a{
    position:absolute;
    left:40px;
    text-decoration:none;
    font-size: 13px;
}
.mid_one_head_center ul li span,.mid_two_head_center ul li span{
    position:absolute;
    right:5px;
    font-size: 14px;
}
.main .mid .mid_three{
    width: 205px;
    height:100%;
    background:url("../images/gonggao1.png") no-repeat center/100% 100%;
    position:absolute;
    right:0px;
}
.main .mid .mid_three .mid_three_head{
    width:100%;
    height: 40px;
    margin-top:-5px;
    background:url("../images/gonggao.png") no-repeat center/100% 100%;
}
.mid_three_head span{
    line-height: 40px;
    margin: 72px;
    font-size: 18px;
}
.mid_three p{
    font-size: 13px;
    line-height: 31px;
    padding:5px;
}
.main .bottom{
    width:100%;
    height: 280px;
    margin-top:20px;
}
.bottom .bottom_head{
    width:100%;
    height: 40px;
    background:url("../images/bottom_head.png") no-repeat center/100% 100%;
}
.bottom_head span{
    line-height: 40px;
    font-size: 15px;
    color:#fff;
    margin-left: 60px;
}
.bottom_main{
    width:100%;
    height: 220px;
    border:1px solid #f5db95;
}
.bottom_main ul{
    padding:0px;
    width: 98%;
    height:100%;
    margin:auto;
    list-style:none;
    display:flex;
    justify-content:space-around;
}
.bottom_main ul li{
    width: 210px;
    height:100%;
}
.bottom_main ul li img{
    width: 100%;
    height:90%;
    margin:10px auto;
}
.bottom_main ul li span{
    display:block;
    width:100%;
    text-align:center;
    font-size: 15px;
}
.bottom_banner{
    width:70%;
    height: 150px;
    margin: 10px auto;
    position:relative;
    background:url("../images/bottom_banner.png") no-repeat center/100% 100%;
}
.bottom_banner_inner{
    width: 55%;
    height: 35%;
    position: absolute;
    top: 50px;
    right: 180px;
    background: url(../images/bottom_inner.png) no-repeat center/100% 100%;
}
.footer{
    width:100%;
    height: 150px;
    margin-top:10px;
    background:url(../images/footer.png) no-repeat center/100% 100%;
    position:relative;
}
.footer span{
    display:block;
    width:100%;
    text-align:center;
    position:absolute;
    top:70px;
    font-size: 16px;
}
.footer .footer_ask{
    width:100%;
    text-align:center;
    position:absolute;
    top:100px;
    font-size: 17px;
}
.active{
   width:215px;
   height: 40px;
   background:url("../images/BL_mid.png") no-repeat center/100% 100%;
   margin:20px auto;
   cursor:pointer;
   font-weight:bold;
}
.backTop{
    position:fixed;
    right:50px;
    bottom:60px;
    width:35px;
    height:35px;
    background:url("../images/backtop.png") no-repeat center/100% 100%;
    display:none;
    z-index:9;
}
