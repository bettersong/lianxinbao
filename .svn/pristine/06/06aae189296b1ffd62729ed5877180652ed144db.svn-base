.header{
    width:100%;
    /* position:fixed; */
    top:0px;
    z-index:9;
}
.head{
     width:100%;
     height: 80px;
     background:url("../images/head1.png") no-repeat center/100% 100%;
     margin-top:0px;
     position:relative;
}
.head .h-inner{
    width:60%;
    height:50%;
    margin:auto;
    background:url("../images/tit.png") no-repeat center/70% 60%;
    position:absolute;
    top:10px;
    left:90px;
}
#menu{
    width:100%;
    height: 25px;
    background:url("../images/daohang.png") no-repeat center/100% 100%;
    margin-top:-25px;
    box-shadow: 0 2px 20px #f9b487;
}
#menu .nav{
    width:370px;
    height:100%;
    margin:0 auto;
    list-style:none;
    display:flex;
    justify-content:space-around;
    padding:0px;
}
#nav .mainlevel{background:#ffe60c;float:left;border-right:1px solid #fff;width:140px;/*IE6 only*/}
.nav .nav_li{
    width: 100%;
    height:100%;
    border-right:1px solid #fff;
    text-align:center;
    align-items: center;
    position:relative;
    cursor:pointer;
}
.nav .nav_li:hover{
    background:#ffefd5;
}
.nav .nav_li:hover a{
    color:#333;
}
.nav .nav_li:hover ul li a{
    color:#fff;
}
// .nav a{
//     text-decoration: none;
//     color:#fff;
//     font-size:14px;
//     font-weight:bold;
// }
.nav_li a{
        text-decoration: none;
        color:#fff;
        display: inline-block;
        width: 100%;
        height: 30px;
        line-height: 25px;
        font-weight:500;
        blr:expression(this.onFocus=this.close());
        blr:expression(this.onFocus=this.blur());
        font-size: 12px;
    }
.nav li a:focus{
    -moz-outline-style: none;
    }
.nav li .sub-nav{
    padding:0px;
    position:absolute;
    top: 30px;
    left: 0;
    list-style:none;
    background-color:#b41d24;
    display: none;
    z-index:9;
    }
.nav li .sub-nav li{
    text-align:center;
    clear:left;
    width: 107px;
    height: 30px;
    line-height: 30px;
    position:relative;
    border-top: 1px solid #fff;
    }
.nav li .sub-nav li:hover{
        background:#ffefd5;
        color:#000;
    }
.nav li .sub-nav li:hover a{

        color:#000;
    }
.nav li .sub-nav li a{
    height:34px;
    line-height:34px;
    width: 107px;
    padding:0;
    display:inline-block;
    font-size: 12px;
    }
.nav_bottom{
    width:100%;
    height: 40px;
    margin-top:180px;
    position:relative;
}
.nav_bottom .nav_bottom_left{
    position:absolute;
    height:100%;
    width: 256px;
    left: 386px;
}
.nav_bottom_left span{
    color:#8b1c3b;
    font-weight:600;
}
#time{
    width: 72%;
    font-size: 14px;
    float: right;
    margin-top: -30px;
}
.nav_bottom .nav_bottom_right{
    position:absolute;
    height:100%;
    width: 260px;
    right: 418px;
    display:flex;
    align-items: center;
}
.nav_bottom .nav_bottom_right .btn{
    width:270px;
    height:60%;
    border: 1px solid #eb4909;
    border-radius: 5px;
    background: #F9F0DA;
    color: #9E9C9C;
}
.nav_bottom .nav_bottom_right .search{
    width:40px;
    height:60%;
    background:#eb4909;
    outline:none;
    display: inline-block;
    // padding: .3em .5em;
    // background-image: linear-gradient(#ddd, #bbb);
    border: 1px solid rgba(0,0,0,.2);
    border-radius: .3em;
    box-shadow: 0 1px white inset;
    text-align: center;
    text-shadow: 0 1px 1px black;
}
.nav_bottom .nav_bottom_right .search:active{
    box-shadow: .05em .1em .2em rgba(0,0,0,.6) inset;
    border-color: rgba(0,0,0,.3);
    background: #eb4333;
}
.main{
    max-width: 600px;
    margin:20px auto;
}
.main .m_top{
    width:300px;
    height: 590px;
    margin:0 auto;
    position:relative;
}
.main .m_top .m_banner{
    width:300px;
    height:100%;
    position:absolute;
    top:0px;
    left:0px;
}
.main .m_top .m_top_right{
    height:350px;
    width: 300px;
    position:absolute;
    top:230px;
    margin:0 auto;
    background:url("../images/tit1.png") no-repeat center/100% 100%;
}
.main .m_top .m_top_right .m_top_right_title{
    width:100%;
    height:40px;
