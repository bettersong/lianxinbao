.B_main{
    width:1024px;
    /* height: 720px; */
    margin:30px auto;
    position:relative;
    overflow: hidden;
}
.B_main .B_main_left{
    width: 255px;
    height:300px;
    position:absolute;
    top:0px;
    left:0px;
    border:1px solid #ffeacb;
    display: inline-block;
    vertical-align: middle;
}
.B_main_left .BL_head{
    width:100%;
    height: 40px;
    background:url("../images/BL_bg.png") no-repeat center/100% 100%;
}
 .BL_head span{
    display: block;
    font-size: 18px;
    font-weight: bold;
    color:#ad2026;
    line-height: 40px;
    margin-left: 30px;
}
.B_main_left .BL_one,.B_main_left .BL_two,.B_main_left .BL_three,.B_main_left .BL_four{
   width:85%;
   height: 40px;
   background:url("../images/BL_mid.png") no-repeat center/100% 100%;
   margin:20px auto;
   cursor:pointer;
  -webkit-transition: all .3s ease-out;
  transition: all .4s ease-out;
}
.BL_one span,.BL_two span,.BL_three span,.BL_four span{
    text-decoration:none;
    display: block;
    font-size: 18px;
    color:#ad2026;
    line-height: 40px;
    margin-left: 50px;
}
.ripple-effect {
  position: relative;
  overflow: hidden;
  cursor: pointer;
  vertical-align: middle;
  z-index: 1;
}
.ripple-effect .ripple {
  display: block;
  position: absolute;
  //border-radius: 100%;
  background: rgba(255, 255, 255, 0.5);
  -webkit-transform: scale(0);
  transform: scale(0);
  pointer-events: none;
}
.ripple-effect .animated {
  -webkit-animation: ripple 0.6s linear;
  animation: ripple 0.3s linear;
}
@-webkit-keyframes ripple {
  100% {
    opacity: 0;
    -webkit-transform: scale(3);
    transform: scale(3);
  }
}
@keyframes ripple {
  100% {
    opacity: 0;
    -webkit-transform: scale(3);
    transform: scale(3);
  }
}
.ripple-effect.ripple-red .ripple {
  width:300px;
  background: rgba(244, 67, 54, 0.725) !important;
}

.BL_one a,.BL_two a,.BL_three a,.BL_four a{
    width:100%;
