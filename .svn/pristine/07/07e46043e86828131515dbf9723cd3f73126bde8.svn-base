.register{
    width: 600px;
    margin:0 auto;
    padding: 20px 30px 30px;
    border: 1px solid #ddd;
    border-radius: 5px;
}
.register label{	
	display: inline-block;
	text-align: right;
}
.userRegist label{
	width: 85px;
}
.register label span{
	color:#f30;
	margin:0 2px 0 0
}
.applyAdmin label{width: 98px;}
.loginRegTitle{
	text-align: center;
    margin: 15px auto;
    font-size: 16px;
	
}
.register span.initTip{
	color:#888;
}
.register span.tip{
	color:#f30;
}
 
.register a{
    text-decoration: underline;
    color:#38f;
}
#accountExist,#passwordTip,#phoneNumberTip,#unitExist,#departmentExist,#applyExist{
    display: none;
}
input[type=text],select,input[type=password]{
    width: 232px;
    height: 35px;
    margin-top: 10px;
    padding: 0px 15px;
    border: 1px solid rgba(0, 0, 0, .15);
    border-radius: 6px;
    color: #000000;
    letter-spacing: 2px;
    font-size: 14px;
    background: transparent;
    margin-right: 5px;
}
select{
    width: 266px;
}
input[type=button]{
    cursor:pointer;
    width: 264px;
    padding: 8px 8px;
    margin-top: 10px;
    background:#ef4300;
    border:1px solid #ff730e;
    font-weight:700;
    color:#fff;
    /* font-size: 14px; */
    font-family: heiti;
    text-shadow:0 1px 2px rgba(0, 0, 0, .1);
}
#searchResult{
    border:1px solid red;
}
#searchResult span{
    width: 100px;
    font-size: 20px;
    display: block;
    margin: 0 auto;
    margin-top: 10px;
}
#searchResult a:last-child{
    display: block;
}
